{
  "version": 2,
  "builds": [
    {
      "src": "weather.html",
      "use": "@vercel/static"
    },
    {
      "src": "test-api.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/weather.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/weather/forecast.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/weather.html"
    },
    {
      "src": "/weather.html",
      "dest": "/weather.html"
    },
    {
      "src": "/test",
      "dest": "/test-api.html"
    },
    {
      "src": "/weather",
      "dest": "/api/weather"
    },
    {
      "src": "/weather/forecast",
      "dest": "/api/weather/forecast"
    }
  ]
}
